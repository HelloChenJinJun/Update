<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anew.update">

    <application
        android:name="com.example.commonlibrary.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/CustomTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WheelActivity" />
        <activity android:name=".mvp.othernew.special.SpecialNewsActivity" />
        <activity android:name=".mvp.othernew.detail.OtherNewsDetailActivity" />
        <activity android:name=".mvp.othernew.photo.OtherNewPhotoSetActivity" />
        <activity android:name=".mvp.index.adjust.AdjustNewsTypeActivity" />
        <!--<activity android:name=".WheelActivity" />-->

        <meta-data
            android:name="com.anew.update.base.App"
            android:value="IModuleConfig" />
    </application>

</manifest>